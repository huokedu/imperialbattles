RewriteEngine On

RewriteCond %{SERVER_PORT} ^80$ [OR]
RewriteCond %{HTTPS} !=on
RewriteRule .* https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

Header add Strict-Transport-Security "max-age=157680000"

<FilesMatch "^privateInfo.ini$|^\.htaccess$|\.log$">
    Order allow,deny
    Deny from all
</FilesMatch>